{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        }
      }
    ],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "add-module-exports"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  }
}
